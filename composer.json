{
    "name": "kodjunkie/onesignal-php-sdk",
    "description": "OneSignal SDK for PHP developers with fluent API and supports Laravel / Lumen out of the box.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Onah",
            "email": "paplow01@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Kodjunkie\\OnesignalPhpSdk\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.3|^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodjunkie\\OnesignalPhpSdk\\OneSignalServiceProvider"
            ],
            "aliases": {
                "OneSignal": "Kodjunkie\\OnesignalPhpSdk\\Facade"
            }
        }
    }
}
